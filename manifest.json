{
  "manifest_version": 2,
  "name": "PDF Reader Extension",
  "version": "1.0",
  "description": "Extension to read PDF files and interact with chat.openai.com",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["pdf.min.js", "pdf.worker.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "chat-gpt.png"
  },
  "manifest_version": 2
}
